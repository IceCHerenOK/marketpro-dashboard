# Порт для backend сервера
PORT=3001

# JWT секрет для аутентификации (замените на свой)
JWT_SECRET=your-super-secret-jwt-key-here

# База данных
DATABASE_PATH=./data/marketplace.db

# API ключи маркетплейсов (заполните своими)
WILDBERRIES_API_KEY=your_wildberries_api_key
OZON_CLIENT_ID=your_ozon_client_id
OZON_API_KEY=your_ozon_api_key
YANDEX_MARKET_TOKEN=your_yandex_market_token
MEGAMARKET_API_KEY=your_megamarket_api_key
MAGNITMARKET_API_KEY=your_magnitmarket_api_key

# Настройки приложения
APP_NAME=MarketPro
APP_VERSION=1.0.0

# Режим разработки
NODE_ENV=development